
#include<bits/stdc++.h>
using namespace std;

class Point{
    int x,y;
    
    public:
    Point(int x=0,int y=0): x{x},y{y} {};

    Point operator+(Point &temp)
    {

        Point P1;
        P1.x=x+temp.x;
        P1.y=y+temp.y;

        return P1;
    }
    void Display()
    {
        cout<<x<<"+i"<<y<<endl;
    }


};

int main() {
    Point p1(10,30),p2(39,45);
    Point p3=p1+p2;
    p3.Display();
    return 0;
}
